{
  "name": "koa-getting-started",
  "version": "1.0.0",
  "private": true,
  "description": "A sample Node.js app using Koa",
  "main": "server.js",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "npm-run-all dev:build",
    "prod": "npm-run-all prod:build",
    "dev:dll": "better-npm-run dll:dev",
    "prod:dll": "better-npm-run dll:prod",
    "dev:build": "better-npm-run build:dev",
    "prod:build": "better-npm-run build:prod",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg"
  },
  "betterScripts": {
    "start": {
      "command": "node ./server/server-prod.js",
      "env": {
        "LEANCLOUD_APP_ID": "NApWmr4uyoMKzfKik2pzDWhn-gzGzoHsz",
        "LEANCLOUD_APP_KEY": "5FWkqzh3Rl9cQLtfuFF6rEqX",
        "LEANCLOUD_APP_MASTER_KEY": "nv13ozE9nSvC7s6Yefi9AOsL"
      }
    },
    "dll:dev": {
      "command": "webpack --config build/webpack/webpack.dll.conf.js --progress",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dll:prod": {
      "command": "webpack --config build/webpack/webpack.dll.conf.js --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "node server/server-dev.js",
      "env": {
        "NODE_ENV": "development",
        "LEANCLOUD_APP_ID": "NApWmr4uyoMKzfKik2pzDWhn-gzGzoHsz",
        "LEANCLOUD_APP_KEY": "5FWkqzh3Rl9cQLtfuFF6rEqX",
        "LEANCLOUD_APP_MASTER_KEY": "nv13ozE9nSvC7s6Yefi9AOsL"
      }
    },
    "build:prod": {
      "command": "webpack --config build/webpack/webpack.prod.conf.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "ejs": "^2.5.6",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-ejs": "^4.1.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "koa-views": "^6.0.2",
    "leancloud-storage": "^3.0.2",
    "leanengine": "^3.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-mixins": "^6.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "base-64": "^0.1.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "colors": "^1.1.2",
    "compression-webpack-plugin": "^1.0.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "happypack": "^4.0.0-beta.5",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "intl-messageformat": "^2.1.0",
    "koa-history-api-fallback": "^0.1.3",
    "koa-webpack-dev-middleware": "^2.0.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "koa-webpack-middleware": "^1.0.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^4.0.3",
    "nodemon": "^1.11.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "promise-loader": "^1.0.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-pagan": "^0.2.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.9",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.5.5",
    "webpack-md5-hash": "0.0.5",
    "webpack-parallel-uglify-plugin": "^1.0.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "8.x"
  }
}
